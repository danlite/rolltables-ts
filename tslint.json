{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": true,
    "ordered-imports": true,
    "no-console": false,
    "semicolon": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "quotemark": {
      "options": "double"
    }
  },
  "rulesDirectory": []
}